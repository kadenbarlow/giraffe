{
  "name": "giraffe",
  "version": "0.1.0-alpha",
  "license": "MIT",
  "bin": "dist/cli/index.js",
  "type": "module",
  "engines": {
    "node": ">=16"
  },
  "scripts": {
    "build": "rm -rf ./dist && babel --out-dir=dist src",
    "dev": "npm run build;chmod +x ./dist/cli/index.js; node --inspect=9228 ./dist/cli/index.js",
    "start": "npm run build;chmod +x ./dist/cli/index.js; node ./dist/cli/index.js",
    "lint": "prettier --check src/. && eslint src/.",
    "prepare": "husky"
  },
  "files": [
    "dist"
  ],
  "dependencies": {
    "clipboardy": "^4.0.0",
    "gql-generator": "^2.0.0",
    "graphql": "^16.9.0",
    "graphql-tools": "^9.0.2",
    "highlight.js": "^11.10.0",
    "ink": "^5.0.1",
    "meow": "^11.0.0",
    "parse5": "^5.0.1",
    "parse5-htmlparser2-tree-adapter": "^5.0.1",
    "prettier": "^3.3.3",
    "react": "^18.2.0",
    "tree-sitter": "^0.21.1",
    "zustand": "^5.0.0-rc.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.21.0",
    "@babel/preset-react": "^7.18.6",
    "@eslint/js": "^9.11.0",
    "@trivago/prettier-plugin-sort-imports": "^4.3.0",
    "@types/react": "^18.3.9",
    "babel-plugin-module-resolver": "^5.0.2",
    "chalk": "^5.2.0",
    "eslint-config-xo-react": "^0.27.0",
    "eslint-plugin-perfectionist": "^3.6.0",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-sort-destructure-keys": "^2.0.0",
    "husky": "^9.1.6",
    "lint-staged": "^15.2.10"
  },
  "babel": {
    "presets": [
      "@babel/preset-react"
    ],
    "plugins": [
      [
        "module-resolver",
        {
          "root": [
            "./src"
          ],
          "alias": {
            "#components": "./src/components",
            "#components/*": "./src/components/*",
            "#features": "./src/features",
            "#features/*": "./src/features/*",
            "#hooks": "./src/hooks",
            "#hooks/*": "./src/hooks/*",
            "#lib": "./src/lib",
            "#lib/*": "./src/lib/*",
            "#stores": "./src/stores",
            "#stores/*": "./src/stores/*"
          }
        }
      ]
    ]
  }
}
